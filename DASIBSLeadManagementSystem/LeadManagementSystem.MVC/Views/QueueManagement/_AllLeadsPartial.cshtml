<div class="card">
    <div class="card-body">
        <div class="d-sm-flex d-block align-items-center justify-content-between mb-9">
            <h4 class="card-title fw-semibold mb-0">All Leads</h4>
            <div class="mb-3 mb-sm-0">
                <div class="d-sm-flex d-block align-items-center justify-content-between">
                    <div class="form-group d-sm-flex d-block align-items-center justify-content-between mx-3">
                        <div class="position-relative">
                            <input type="text" class="form-control product-search ps-5" id="input-search" placeholder="Search By Name...">
                            <span class="position-absolute top-50 start-0 translate-middle-y fs-6 text-dark ms-3">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-search"><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M10 10m-7 0a7 7 0 1 0 14 0a7 7 0 1 0 -14 0" /><path d="M21 21l-6 -6" /></svg>
                            </span>
                        </div>
                    </div>
                    <button type="button" class="btn bg-light text-dark" data-bs-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
                        <img src="~/images/adjustments-alt.svg" alt="New" width="20" data-bs-toggle="tooltip" data-bs-placement="top" data-bs-title="Advanced Search" />
                        Filter
                    </button>
                </div>
            </div>
        </div>
        <div class="collapse" id="collapseExample">
            <div class="border-top py-3 border-bottom mb-4 bg-light px-2">
                <form class="row">
                    <div class="col-2">
                        <div class="mb-3">
                            <select class="form-select bg-white" id="technicalClass" name="technicalClass">
                                <option>Lead Source</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-3">
                            <input type="text" class="form-control bg-white" id="name" name="name" placeholder="Lead Number">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-3">
                            <input type="text" class="form-control bg-white" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="Status Date">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-3">
                            <input type="number" class="form-control bg-white" id="Number" name="Number" placeholder="ID Number">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-3">
                            <select class="form-select bg-white" id="Status" name="Status">
                                <option>Status</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-3">
                            <select class="form-select bg-white" id="Agent" name="Agent">
                                <option>Agent</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <input type="text" class="form-control bg-white" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="Date From">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <input type="text" class="form-control bg-white" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="Date To">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <select class="form-select bg-white" id="Dalas Status State" name="Dalas Status State">
                                <option>Dalas Status State</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <input type="number" class="form-control bg-white" id="Alternate Number" name="Alternate Number" placeholder="Alternate Number">
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <select class="form-select bg-white" id="Closed" name="Closed">
                                <option>Closed</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="mb-4">
                            <select class="form-select bg-white" id="Ping Result" name="Ping Result">
                                <option>Ping Result</option>
                            </select>
                        </div>
                    </div>


                    <div class="col-3">
                        <div class="">
                            <button type="button" class="btn bg-primary text-white">
                                <img src="~/images/search-white.svg" alt="Search" width="20" />
                                Search
                            </button>
                            <button type="button" class="btn btn-outline-dark mx-2">
                                Reset
                            </button>
                        </div>
                    </div>
                    <div class="col">
                        <div class="">
                        </div>
                    </div>
                </form>
            </div>
        </div>



